  <script type="text/javascript">
  function getCookie(name) {
    var cookies = document.cookie;
    var prefix = name + "=";
    var begin = cookies.indexOf("; " + prefix);
    if (begin == -1) {
      begin = cookies.indexOf(prefix);
      if (begin != 0) {
        return null;
      }
    } else {
      begin += 2;
    }
    var end = cookies.indexOf(";", begin);
    if (end == -1) {
      end = cookies.length;
    }
    return unescape(cookies.substring(begin + prefix.length, end));
  }

  function cadastro(a) {
    if (getCookie('IPI') !== null) {
      var url1 = getCookie('IPI');
      window.location.assign("https://cadastro.rommanel.com.br/?" + url1);
    } else {
      window.location.assign("https://cadastro.rommanel.com.br/");
    }
  }
</script>
